{
  "name": "@patys/snap-it",
  "version": "0.2.0",
  "description": "Tool to generate jest test cases with snapshots for React Native components.",
  "main": "index.ts",
  "author": "Patryk Szczyg≈Ço",
  "license": "MIT",
  "bin": {
    "snap-it": "bin/snap-it.js"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "build": "babel --extensions .ts,.tsx src --out-dir lib --ignore '**/__tests__/**' --source-maps --delete-dir-on-start",
    "start": "yarn build && node lib/index.js",
    "lint": "./node_modules/.bin/tslint --project ./tsconfig.json",
    "release": "release-it"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.1",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@release-it/conventional-changelog": "^2.0.0",
    "@types/jest": "^25.1.2",
    "@types/node": "^14.14.6",
    "husky": "^4.3.0",
    "prettier": "2.1.2",
    "release-it": "^14.2.1"
  },
  "dependencies": {
    "@babel/core": "^7.12.3",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-flow": "^7.12.1",
    "@babel/preset-react": "^7.12.5",
    "@babel/preset-typescript": "^7.12.1",
    "chalk": "^4.1.0",
    "ejs": "^3.1.5",
    "fs-extra": "^9.0.1",
    "ts-jest": "^26.4.3",
    "tsc-watch": "^4.2.9",
    "tslint": "^6.1.3",
    "typescript": "^4.0.5",
    "yargs": "^16.1.0"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
