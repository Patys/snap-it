{
  "name": "@patys/snap-it",
  "version": "0.2.0",
  "description": "Tool to generate jest test cases with snapshots for React Native components.",
  "main": "index.ts",
  "author": "Patryk Szczyg≈Ço",
  "license": "MIT",
  "bin": {
    "snap-it": "bin/snap-it.js"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "build": "babel --extensions .ts,.tsx src --out-dir lib --ignore '**/__tests__/**' --source-maps --delete-dir-on-start",
    "start": "yarn build && node lib/index.js",
    "lint": "./node_modules/.bin/tslint --project ./tsconfig.json",
    "release": "release-it"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@release-it/conventional-changelog": "^1.1.0",
    "@types/jest": "^25.1.2",
    "@types/node": "^13.7.1",
    "husky": "^4.2.3",
    "prettier": "2.0.2",
    "release-it": "^13.3.0"
  },
  "dependencies": {
    "@babel/core": "^7.8.4",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/preset-env": "^7.8.4",
    "@babel/preset-flow": "^7.8.3",
    "@babel/preset-react": "^7.8.3",
    "@babel/preset-typescript": "^7.8.3",
    "chalk": "^3.0.0",
    "ejs": "^3.0.1",
    "fs-extra": "^9.0.0",
    "ts-jest": "^25.2.0",
    "tsc-watch": "^4.1.0",
    "tslint": "^6.0.0",
    "typescript": "^3.7.5",
    "yargs": "^15.3.1"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
